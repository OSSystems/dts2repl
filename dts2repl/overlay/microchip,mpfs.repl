clint:
    numberOfTargets: 5
    frequency: 1000000

plic:
    prioritiesEnabled: false

mstimer: Timers.MPFS_Timer @ sysbus 0x20125000
    Timer1IRQ -> plic@82
    Timer2IRQ -> plic@83

cpu0:
    privilegeArchitecture: PrivilegeArchitecture.Priv1_10
    hartId: 0

cpu1:
    privilegeArchitecture: PrivilegeArchitecture.Priv1_10
    hartId: 1
