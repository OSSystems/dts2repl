gic: IRQControllers.ARM_GenericInterruptController @ {
        sysbus new Bus.BusMultiRegistration { address: 0xf9010000; size: 0x1000; region: "distributor" };
        sysbus new Bus.BusMultiRegistration { address: 0xf9020000; size: 0x100; region: "cpuInterface" }
    }
    0 -> cpu0@0
    numberOfCPUs: 1

mem: Memory.MappedMemory @ sysbus 0xfff00000
    size: 0x100000
